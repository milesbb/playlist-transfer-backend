{
  "name": "playlist-transfer-backend",
  "version": "1.0.0",
  "description": "Backend for playlist transfer SaaS",
  "main": "lambda.js",
  "scripts": {
    "clean": "node ./scripts/clean-dist.js",
    "start-local-db": "node ./scripts/setup-local-db.js",
    "cleanup-db": "node ./scripts/cleanup-local-db.js",
    "dev": "npm run clean && npm run start-local-db && npm run start-local",
    "build": "npm run clean && webpack --config webpack.config.js",
    "start-local": "webpack --config webpack.local.config.js --watch & nodemon dist/local-server.js",
    "start": "node dist/lambda.js",
    "start-offline-serverless": "sls offline start",
    "lint": "eslint 'src/**/*.ts' --ext .ts",
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint-type-check": "npm run lint && npm run check-types",
    "vitest": "vitest",
    "test": "npm run lint-type-check && npm run vitest",
    "test-wl": "npm run lint-type-check && npm run vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesbb/playlist-transfer-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milesbb/playlist-transfer-backend/issues"
  },
  "homepage": "https://github.com/milesbb/playlist-transfer-backend#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "npm": "^11.6.0",
    "prettier": "^3.6.2",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.888.0",
    "argon2": "^0.44.0",
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "serverless-http": "^4.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  }
}
